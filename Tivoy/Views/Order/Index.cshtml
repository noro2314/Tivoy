@using X.PagedList.Mvc.Core;
@using X.PagedList;
@model X.PagedList.IPagedList<DataModels.OrderViewModel>
<div class="card">
    <div class="card-header">
        <div class="row">
            <div class="col-md-10">
                <input placeholder="Search" class="col-md-3 form-control" type="text" id="Search" name="Search">
                <i class="col-md-3 form-group__bar"></i>
            </div>
            <div class="col-md-2">
                <a asp-action="Create" asp-controller="Order">
                    <i class="zmdi zmdi-apps"></i>Add New Order</a>
            </div>
        </div>
    </div>
</div>
<div class="card">
    <div class="card-header">
        <div class="row" id="divList">
            @await Html.PartialAsync("_List", Model)
        </div>
    </div>
</div>